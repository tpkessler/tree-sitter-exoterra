{
  "name": "tree-sitter-exoterra",
  "version": "0.1.0",
  "description": "Tree sitter grammar for terra witht language extensions",
  "repository": "github:tree-sitter/tree-sitter-exoterra",
  "license": "MIT",
  "author": {
    "name": "Torsten Ke√üler <t.kessler@posteo.de>",
    "email": "t.kessler@posteo.de"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "exoterra"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.1.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.3",
    "tree-sitter-terra": "github.com/tpkessler/tree-sitter-terra-rewrite"
    "@tree-sitter-grammars/tree-sitter-lua": "^0.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "source.exoterra",
      "injection-regex": "^exoterra$"
    }
  ]
}
